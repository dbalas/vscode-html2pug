{
    "name": "vscode-html2pug",
    "displayName": "html2pug",
    "description": "HTML to PUG VSCode extension",
    "version": "0.0.2",
    "icon": "icon.png",
    "bugs": {
        "url": "https://github.com/dbalas/vscode-html2pug/issues",
        "email": "dbalasdev@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/dbalas/vscode-html2pug.git"
    },
    "license": "MIT",
    "publisher": "dbalas",
    "engines": {
        "vscode": "^1.25.0"
    },
    "tags": [
        "vscode",
        "jade",
        "pug",
        "html2pug",
        "html2jade"
    ],
    "categories": [
        "Programming Languages",
        "Formatters",
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.transformHtmlToPug"
    ],
    "main": "./extension",
    "contributes": {
        "keybindings": [
            {
                "command": "extension.transformHtmlToPug",
                "key": "ctrl+alt+p",
                "mac": "cmd+alt+p",
                "linux": "ctrl+alt+p"
            }
        ],
        "commands": [
            {
                "command": "extension.transformHtmlToPug",
                "title": "html2pug: Transform"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^5.2.5",
        "@types/node": "^10.9.4",
        "eslint": "^5.5.0",
        "typescript": "^3.0.3",
        "vscode": "^1.1.21"
    },
    "dependencies": {
        "html2pug": "^2.0.1"
    }
}
